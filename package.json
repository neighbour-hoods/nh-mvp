{
  "name": "nh-paper-widget",
  "version": "1.0.0",
  "description": "",
  "main": "ui/main.ts",
  "directories": {
    "ui": "ui"
  },
  "dependencies": {
    "@holochain-open-dev/cell-client": "^0.3.3",
    "@holochain-open-dev/core-types": "^0.2.0",
    "@holochain/client": "^0.3.2",
    "source-map-loader": "^4.0.0",
    "ts-loader": "^9.3.0",
    "typescript-logging": "^2.0.1",
    "typescript-logging-category-style": "^2.0.1",
    "vue": "^3.2.26",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "scripts": {
    "start": "miniserve .",
    "fe:build": "webpack-cli --config webpack.config.js",
    "ui": "npm run fe:build && npm run start",
    "fe:build-prod": "webpack-cli --entry ./ui/main.ts --output-filename index.js --mode production",
    "hc:clean": "rm -rf .hc*",
    "hc:build-paperz": "cargo build -p paperz --release --target wasm32-unknown-unknown && cp $CARGO_TARGET_DIR/wasm32-unknown-unknown/release/paperz.wasm ./happs/paperz",
    "hc:pack-paperz": "hc dna pack happs/paperz && hc app pack happs/paperz",
    "hc:run-paperz": "hc sandbox generate happs/paperz && WASM_LOG=debug RUST_LOG=debug RUST_BACKTRACE=full hc sandbox -f=9000 run -l -p 9999",
    "hc:redo-paperz": "npm run hc:build-paperz && npm run hc:pack-paperz && npm run hc:run-paperz",
    "hc:mk-sensemaker": "nix build .#social_sensemaker --impure",
    "hc:pg": "npx @holochain-playground/cli ws://localhost:9000"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/vue": "^2.0.0"
  }
}
